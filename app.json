{
  "name": "discord-invite-bot",
  "options": {
    "allow-unauthenticated": true,
    "memory": "256Mi",
    "cpu": "1",
    "port": 3000,
    "http2": false
  },
  "env": {
    "NEXT_PUBLIC_RECAPTCHA_SITE_KEY": {
      "description": "Public Site Key for reCAPTCHA",
      "required": true
    },
    "RECAPTCHA_SECRET_KEY": {
      "description": "Private Key for reCAPTCHA"
    },
    "DISCORD_BOT_TOKEN": {
      "description": "Discord Bot Token"
    },
    "DISCORD_CHANNEL_ID": {
      "description": "Channel ID invite should send user to"
    },
    "NEXT_PUBLIC_APP_NAME": {
      "description": "Title for your App"
    }
  }
}
